server:
  grpc:
    addr: 0.0.0.0:9000
    timeout: 1s
data:
  storage:
    endpoint: ""
    region: ""
    bucket: "ipfs-blocks-xxxx"
    accessKey: ""
    secretKey: ""
  db:
    type: LevelDB
    leveldb:
      path: ./db
    tikv:
      addrs:
        - 127.0.0.1:2379
    pg:
      dsn: host=127.0.0.1 user=postgres password=password dbname=ipfs port=5432 sslmode=disable TimeZone=UTC
  cache:
    lruSize: 64
    basePath: "./cache"
    indexPath: "./cache-index"
  redis:
    addr: 127.0.0.1:6379
    password: password
    read_timeout: 0.2s
    write_timeout: 0.2s

